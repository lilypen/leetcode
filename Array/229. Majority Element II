class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        if(nums.size()<2) return nums;
        int num1,num2,count1=0,count2=0;
        for(int num:nums)
        {
            if(count1==0&&num!=num2)
            {
                num1=num;
                count1++;
            }
            else if(count2==0&&num!=num1)
            {
                num2=num;
                count2++;
            }
            else if(num==num1)
            {
                count1++;
                //count2--;
            }
            else if(num==num2)
            {
                //count1--;
                count2++;
            }
            else
            {
                count1--;
                count2--;
            }
        }
        count1=0,count2=0;
        vector<int> res;
        for(int num:nums)
        {
            if(num==num1) count1++;
            if(num==num2) count2++;
        }

        if(count1>(nums.size())/3) res.push_back(num1);
        if(count2>(nums.size())/3) res.push_back(num2);
        
        return res;
        
    }
};
